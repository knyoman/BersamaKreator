{
  "name": "bersamakreator-app",
  "version": "1.0.0",
  "description": "BersamaKreator Application",
  "build": {
    "command": "npm install && npm run build",
    "output": "dist"
  },
  "functions": {
    "directory": "node-functions",
    "runtime": "nodejs",
    "routes": [
      {
        "path": "/ai-match",
        "method": ["POST", "OPTIONS"],
        "function": "ai-match"
      },
      {
        "path": "/create-order",
        "method": ["POST", "OPTIONS"],
        "function": "create-order"
      }
    ]
  },
  "caches": [
    {
      "source": "/images/*",
      "cacheTtl": 86400
    }
  ]
}

